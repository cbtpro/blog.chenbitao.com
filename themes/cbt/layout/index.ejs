<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no"/>
        <title><%= config.title %></title>
        <%- css('//cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.min.css') %>
        <%- css('//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap-theme.min.css') %>
        <%- css('//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css') %>
        <%- css('/styles/theme.css') %>
        <%- js('//cdn.bootcss.com/angular.js/1.4.12/angular.min.js') %>
        <%- js('//cdn.bootcss.com/angular.js/1.4.12/i18n/angular-locale_zh-cn.min.js') %>
        <%- js('//cdn.bootcss.com/jquery/3.1.0/jquery.min.js') %>
        <%- js('//cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js') %>
        <%- js('//cdn.bootcss.com/moment.js/2.14.1/moment.min.js') %>
        <%- js('//cdn.bootcss.com/moment.js/2.14.1/locale/zh-cn.js') %>
        <%- js('//cdn.bootcss.com/d3/4.2.2/d3.min.js') %>
        <%- js('//cdn.bootcss.com/socket.io/1.4.8/socket.io.min.js') %>
    </head>
    <body>
        <div id="container">
            <%- partial('./common/header', {index: true}) %>
            <% page.posts.each(function(post, index){ %>
                <%- partial('./article', {index: true, post: post}) %>
            <% }) %>
        </div>
    </body>
</html>